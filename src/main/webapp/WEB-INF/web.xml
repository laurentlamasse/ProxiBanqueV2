  	<servlet-class>com.gtm.proxibanquev2.presentation.AjoutClientServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AjoutCompteCourant</servlet-name>
  	<display-name>AjoutCompteCourant</display-name>
  	<description></description>
  	<servlet-class>com.gtm.proxibanquev2.presentation.AjoutCompteCourant</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ListeClientsServlet</servlet-name>
