  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>VirementServlet</servlet-name>
  	<url-pattern>/VirementServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AjoutClientServlet</servlet-name>
  	<url-pattern>/AjoutClientServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AjoutCompteCourant</servlet-name>
