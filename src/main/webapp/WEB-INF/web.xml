  	<servlet-class>com.gtm.proxibanquev2.presentation.VirementServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AjoutClientServlet</servlet-name>
  	<display-name>AjoutClientServlet</display-name>
