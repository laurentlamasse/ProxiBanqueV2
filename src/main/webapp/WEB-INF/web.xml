  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ListeClientsServlet</servlet-name>
  	<url-pattern>/ListeClientsServlet</url-pattern>
  </servlet-mapping>
  <error-page>
	  <error-code>404</error-code>
	  <location>/error.jsp</location>
  </error-page>
  <error-page>
	  <exception-type>java.lang.Exception</exception-type>
	  <location>/error.jsp</location>
  </error-page>
